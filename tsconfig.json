{
    "files": [],
    "references": [
        { "path": "./tsconfig.app.json" },
        { "path": "./tsconfig.node.json" }
    ],
    "compilerOptions": {
        "forceConsistentCasingInFileNames": true,
        "baseUrl": "./",
        "strict": true,
        "paths": {
